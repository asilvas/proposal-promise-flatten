{
  "name": "@asilvas/promise-flatten",
  "version": "0.1.0",
  "description": "TC39 Proposal for `Promise.prototype.flatten` function",
  "main": "polyfill.js",
  "scripts": {
    "build": "nyc mkdir -p out && ecmarkup spec/index.html --js=out/spec.js --css=out/spec.css out/index.html",
    "cover": "nyc ava --verbose",
    "prepublish": "npm run cover",
    "test": "ava --verbose",
    "watch": "nyc mkdir -p out && ecmarkup spec/index.html --js=out/spec.js --css=out/spec.css out/index.html --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asilvas/proposal-promise-flatten.git"
  },
  "keywords": [
    "ecmascript",
    "tc39"
  ],
  "files": [
    "polyfill.js",
    "README.md"
  ],
  "author": "Aaron Silvas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asilvas/proposal-promise-flatten/issues"
  },
  "homepage": "https://github.com/asilvas/proposal-promise-flatten#readme",
  "devDependencies": {
    "ava": "^1.4.1",
    "ecmarkup": "^3.16.0",
    "nyc": "^14.0.0"
  }
}
